{
    "name": "typescript-eslint-prettier-jest-example",
    "version": "0.1.0",
    "description": "Example of Combining eslint prettier jest.",
    "main": "./build/bin/cli.js",
    "types": "./build/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexrecuenco/typescript-eslint-prettier-jest-example.git"
    },
    "bin": "./build/bin/cli.js",
    "scripts": {
        "start": "npm run build && node .",
        "build": "npm run build:debug",
        "build:debug": "tsc",
        "build:prod": "tsc --build tsconfig.prod.json",
        "watch": "tsc-watch --onSuccess 'node .'",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
        "test": "jest"
    },
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "jest",
        "test",
        "datetime",
        "internationalization"
    ],
    "devDependencies": {
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.18",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-prettier": "^4.0.0",
        "fast-check": "^2.22.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "supports-color": "^9.2.1",
        "ts-jest": "^27.1.3",
        "tsc-watch": "^4.6.0",
        "typescript": "^4.5.5"
    }
}
